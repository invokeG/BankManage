<Application x:Class="BankManage.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Main.xaml">
    <Application.Resources>
        
        <DataTemplate x:Key="workDateTemplate">
            <TextBlock Text="{Binding workDate,StringFormat={}{0:yyyy-MM-dd}}"></TextBlock>
        </DataTemplate>
        <DataTemplate x:Key="photoTemplate">
            <Image Source="{Binding Photo}" Width="20" Height="20"></Image>

        </DataTemplate>
        <DataTemplate x:Key="DealDateTemplate">
        </DataTemplate>
    </Application.Resources>

</Application>
